		 */
		ArrayList<Star> stars = new ArrayList<Star>();
		stars.add(new Star("Ploopy Star", 0));
		stars.add(new Star("Galifrey Star", 1));
		stars.add(new Star("Venus Star", 2));
		stars.add(new Star("Uranus Star", 3));
		stars.add(new Star("Jupiter Star", 4));
		for (int i = 0; i < stars.size(); i++) {
			System.out.println(stars.get(i));
		}

	}
